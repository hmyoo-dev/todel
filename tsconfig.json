{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "lib": ["DOM", "ES2019"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "noImplicitOverride": true
  }
}
